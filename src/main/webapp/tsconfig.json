{
	"compileOnSave": true,
    "compilerOptions": {
		"target": "ES5",
        "module": "amd",
        "noImplicitAny": true,
		"strictNullChecks": true,
        "removeComments": true,
        "preserveConstEnums": true,
		"declaration": false,
        "sourceMap": false,
		"jsx": "react",
		"outFile": "./js/app.js"
    },
    "include": [
        "app/",
		"typings/index.d.ts"
    ],
    "exclude": [
		"js/"
    ]
}